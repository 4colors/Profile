{
    "name": "zikula/profile-module",
    "version": "2.1.0",
    "description": "User profiles and member list",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Zikula Development Team",
            "homepage": "http://zikula.org"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ProfileModule\\": "" }
    },
    "require": {
        "php": ">5.4.0"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ProfileModule\\ZikulaProfileModule",
            "core-compatibility": ">=1.4.1",
            "displayname": "Profile",
            "url": "search",
            "capabilities": {
                "admin": {"route": "zikulaprofilemodule_config_config"},
                "user": {"route": "zikulaprofilemodule_profile_display"},
                "profile": {"version": "1.0"}
            },
            "securityschema": {
                "ZikulaSearchModule::": "Module name::",
                "ZikulaSearchModule::": "::",
                "ZikulaSearchModule::view": "::",
                "ZikulaSearchModule::item": "DynamicUserData PropertyName::DynamicUserData PropertyID",
                "ZikulaSearchModule:Members": "::",
                "ZikulaSearchModule:Members:recent": "::",
                "ZikulaSearchModule:Members:online": "::"
            }
        }
    }
}
